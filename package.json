{
  "name": "infomaniak-uploader",
  "version": "0.0.0",
  "private": true,
  "description": "Uploads files securely to Infomaniak-hosted FTP servers using the Infomaniak FTP Manager APIs over HTTPS",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "pnpm build && pnpm lint && pnpm package",
    "test-all": "pnpm build && pnpm lint && pnpm package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rokeller/infomaniak-uploader.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "axios": "^1.9.0",
    "axios-cookiejar-support": "^6.0.2",
    "form-data": "^4.0.3",
    "http-cookie-agent": "7.0.1",
    "tough-cookie": "^5.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@jest/globals": "^30.0.2",
    "@types/node": "^22.0.0",
    "@types/tough-cookie": "^4.0.5",
    "@vercel/ncc": "^0.38.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.0.1",
    "globals": "^16.2.0",
    "jest": "^27.2.5",
    "prettier": "^3.5.3",
    "ts-jest": "^27.1.2",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.34.1"
  }
}
