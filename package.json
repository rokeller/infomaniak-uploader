{
  "name": "infomaniak-uploader",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "description": "Uploads files securely to Infomaniak-hosted FTP servers using the Infomaniak FTP Manager APIs over HTTPS",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "bundle": "pnpm format:write && pnpm package",
    "format:write": "prettier --write '**/*.ts'",
    "format:check": "prettier --check '**/*.ts'",
    "lint": "eslint",
    "local-action": "node __tests__/local-test-generate.js && npx @github/local-action . src/main.ts .env",
    "package": "npx rimraf ./dist && npx rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "all": "pnpm build && pnpm lint && pnpm package",
    "test-all": "pnpm build && pnpm lint && pnpm package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rokeller/infomaniak-uploader.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.3",
    "axios": "^1.13.6",
    "axios-cookiejar-support": "^6.0.5",
    "form-data": "^4.0.5",
    "tough-cookie": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^30.2.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.13",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.2",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.15.0",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "rollup": "^4.59.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1"
  }
}
