{
  "name": "infomaniak-uploader",
  "version": "0.0.0",
  "private": true,
  "description": "Uploads files securely to Infomaniak-hosted FTP servers using the Infomaniak FTP Manager APIs over HTTPS",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn lint && yarn package",
    "test-all": "yarn build && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rokeller/infomaniak-uploader.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "axios": "^1.7.9",
    "axios-cookiejar-support": "^5.0.5",
    "form-data": "^4.0.1",
    "http-cookie-agent": "6.0.8",
    "tough-cookie": "^5.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/eslint__js": "^9.14.0",
    "@types/node": "^22.0.0",
    "@types/tough-cookie": "^4.0.5",
    "@vercel/ncc": "^0.38.3",
    "dotenv": "^16.3.2",
    "eslint": "^9.20.1",
    "eslint-plugin-github": "^5.1.8",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^15.14.0",
    "jest": "^27.2.5",
    "prettier": "^3.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.24.1"
  }
}
