{
  "name": "infomaniak-uploader",
  "version": "0.0.0",
  "private": true,
  "description": "Uploads files securely to Infomaniak-hosted FTP servers using the Infomaniak FTP Manager",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run lint && npm run package",
    "test-all": "npm run build && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rokeller/infomaniak-uploader.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "axios": "^1.4.0",
    "axios-cookiejar-support": "^4.0.6",
    "form-data": "^4.0.0",
    "http-cookie-agent": "^5.0.4",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.1.1",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/parser": "^5.60.0",
    "@vercel/ncc": "^0.36.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-jest": "^27.2.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.8",
    "ts-jest": "^27.1.2",
    "typescript": "^4.9.5"
  }
}
