{
  "name": "infomaniak-uploader",
  "version": "0.0.0",
  "private": true,
  "description": "Uploads files securely to Infomaniak-hosted FTP servers using the Infomaniak FTP Manager APIs over HTTPS",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "pnpm build && pnpm lint && pnpm package",
    "test-all": "pnpm build && pnpm lint && pnpm package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rokeller/infomaniak-uploader.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "axios": "^1.13.2",
    "axios-cookiejar-support": "^6.0.5",
    "form-data": "^4.0.5",
    "http-cookie-agent": "7.0.3",
    "tough-cookie": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "@types/node": "^24.10.1",
    "@types/tough-cookie": "^4.0.5",
    "@vercel/ncc": "^0.38.4",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.2.0",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  }
}
